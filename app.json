{
  "expo": {
    "name": "TriviaRush",
    "slug": "triviarush-game",
    "version": "0.1.0",
    "scheme": "triviarush-game",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-google-signin/google-signin",
      "expo-router",
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/ic_launcher.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#1C1C1E"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.triviarush.mobile"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "icon": "./assets/ic_launcher.png",
        "foregroundImage": "./assets/ic_launcher_foreground.png",
        "monochromeImage": "./assets/ic_launcher_monochrome.png",
        "backgroundImage": "./assets/ic_launcher_background.png"
      },
      "package": "com.triviarush.mobile",
      "versionCode": 1
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "baafd255-7b10-4230-a086-62318555c317"
      }
    },
    "owner": "thegideona"
  }
}
