{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES6",
    "moduleResolution": "Node",
    // "strict": true,
    "jsx": "react-native",
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "~/*": ["./*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts",]
}
